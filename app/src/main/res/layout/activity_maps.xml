<fragment xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:map="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/map"
    android:name="com.google.android.gms.maps.SupportMapFragment"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.itvillage.tms.tms_transportmanagementsystem.MapsActivity" >
    <Button
        android:id="@+id/finishTrip"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_gravity="bottom"
        android:layout_margin="20dp"
        android:background="@drawable/sign_in_but_bg"
        android:fontFamily="@font/philosopher_bold"
        android:text="Finish Trip"
        android:textAllCaps="false"
        android:textColor="@color/colorWhite"
        android:textSize="22dp"
        android:textStyle="bold" />
</fragment>
